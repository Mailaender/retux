msgstr "Novan Ludon"

# Main menu option; shows the Load Game menu
msgid "Load Game"
msgstr "Ŝargi Ludon"

# Main menu option; shows the levelset selection menu
msgid "Select Levelset"
msgstr "Elekti Nivelaron"

# Main menu option; shows the Options menu
msgid "Options"
msgstr "Agordoj"

# Main menu option; shows the credits screen
msgid "Credits"
msgstr "Agnoskoj"

# Main menu option; quits the game
msgid "Quit"
msgstr "Eliri"

# Menu option; indicates that a save slot is empty
msgid "-Empty-"
msgstr "-Malplena-"

# Menu option; indicates that a save slot is not using any levelset.
