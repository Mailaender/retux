msgstr "Anstataŭigi ĉi tian dosieron"

# Menu option; cancel overwriting the chosen save file and return to previous menu
msgid "Cancel"
msgstr "Nuligi"

# Error message shown when a levelset does not properly start.
msgid "An error occurred when trying to load the game."
msgstr "Eraro okazis dum la ŝargo de la ludo."

# Menu option; used to change to the next "page" (section) of the current menu
msgid "Next page"
msgstr "Sekva paĝo"

# Used as a part of binary config menu options below.
msgid "On"
msgstr "Ek"

# Used as a part of binary config menu options below.
msgid "Off"
msgstr "For"

# Menu option; indicates whether or not fullscreen should be used.
# {} is replaced by "On" or "Off".
msgid "Fullscreen: {}"
msgstr "Plena ekraro: {}"

# Menu option; indicates what scale method ReTux should use.
# {} is replaced by the name of a scale method.
msgid "Scale Method: {}"
msgstr "Skalo-metedo: {}"

# Menu option; indicates whether or not sound effects should play.
# {} is replaced by "On" or "Off".
msgid "Sound: {}"
msgstr "Sono: {}"

# Menu option; indicates whether or not music should play.
# {} is replaced by "On" or "Off".
msgid "Music: {}"
msgstr "Muziko: {}"

# Menu option; indicates whether or not stereo sound should be used.
# {} is replaced by "On" or "Off".
msgid "Stereo: {}"
